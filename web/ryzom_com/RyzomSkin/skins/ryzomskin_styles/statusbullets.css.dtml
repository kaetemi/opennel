/* PROGRESS INFO */

div.progress_not_done, div.progress_worked_on, div.progress_completed
{
	background : url(/images/progress/amber_red.gif) no-repeat center left;
	padding-left : 30px;
	margin-left : 20px;
	margin-bottom : 15px;
}

div.progress_worked_on
{
	background : url(/images/progress/amber_orange.gif) no-repeat center left;
}

div.progress_completed
{
	background : url(/images/progress/amber_green.gif) no-repeat center left;
}
