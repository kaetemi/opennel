/*css serverstatus*/
#serverstatus  {
    background-color :  #5f5e57;
    color: #ccc;
    border-radius: 0 0 0 12px;
    -moz-border-radius: 0 0 0 12px;
    padding : 0px 5px 0px 10px;
    float:right;
}
#toplinks  {
   color : #ffc30e;
   background-color :  #5f5e57;
   border-radius: 12px 0 0 0;
   -moz-border-radius: 12px 0 0 0;
   padding : 3px 5px 0 10px;
   float:right;
}
.OPEN {
	background: url('server_up.png') no-repeat;
	padding-left : 20px;
}
.CLOSED {
	background: url('server_down.png') no-repeat;
	padding-left : 20px;
}
.LOCKED {
	background: url('server_locked.png') no-repeat;
	padding-left : 20px;
}
/*fin du css serverstatus*/


