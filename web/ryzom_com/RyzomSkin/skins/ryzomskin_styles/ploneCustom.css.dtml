    visibility: visible;
    border: 1px solid #8cacbb;
    color: #5f5e57;
    background-color: white;
    vertical-align: middle;
}

/*navbar tout en haut*/
#navbar
{
	font-family: arial, helvetica, sans-serif;
	border-top: 1px solid #999;
	background: #c3c3c3;
	border-bottom: 2px solid #999;
	font-weight: normal;
	font-size: 100%;
	margin: 0 0 0 0;
	padding: 2px 0 0 0;
	text-align: center;
}

#navbar li
{
	list-style: none;
	display: inline;
}

#navbar a
{
	text-decoration: none;
	border-left: 1px solid #999;
	padding: 0 2px 0 3px;
	color: black;
}

#navbar a.highlight { font-weight: bold; }
#navbar a:hover { color: #666; }

img { 
border :0;
padding :0;
margin:0;
}

