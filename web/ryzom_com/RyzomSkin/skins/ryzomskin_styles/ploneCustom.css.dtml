/*css serverstatus*/
#serverstatus  {
    background-color :  #5f5e57;
    color: #ccc;
    border-radius: 0 0 0 12px;
    -moz-border-radius: 0 0 0 12px;
    padding : 0px 5px 0px 10px;
    float:right;
}

#toplinks  {
   color : #ffc30e;
   background-color :  #5f5e57;
   border-radius: 12px 0 0 0;
   -moz-border-radius: 12px 0 0 0;
   padding : 3px 5px 0 10px;
   float:right;
}

.OPEN {
	background-image: url('server_up.png');
	background-repeat: no-repeat;
	padding-left : 20px;
}

.CLOSED {
	background-image: url('server_down.png');
	background-repeat: no-repeat;
	padding-left : 20px;

}

.LOCKED {
	background-image: url('server_locked.png');
	background-repeat: no-repeat;
	padding-left : 20px;
}

/*fin du css serverstatus*/




.state-private {color: Red;}
.state-visible {color: #74AE0B;}
.state-published {color: #33a6a9;}
.state-pending {color: #FFA500;}
.state-expired {color: Red;}
.syndicated {color: #008000;}
.portletItem{color:#ffc30e;}
.portletItemDetails{color:#ffc30e;}

